{
  "name": "@fdmg/fd-inputs",
  "version": "2.0.4",
  "description": "FD-themed inputs",
  "types": "dist/fd-inputs.d.ts",
  "main": "dist/fd-inputs.js",
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FDMediagroep/fd-ts-react-inputs.git"
  },
  "scripts": {
    "build-demo": "npm run copy-img && npm run copy-demo-html && cross-env NODE_ENV=production webpack --mode=production --config webpack.demo.config.js",
    "build-dev": "webpack --mode=development",
    "build-prod": "cross-env NODE_ENV=production webpack --mode=production",
    "copy-demo-html": "cpx -p -v \"demo/**/*.html\" \"dist/\"",
    "copy-img": "cpx -p -v \"img/**/*\" \"dist/\"",
    "lint": "tslint -p .",
    "release": "npm update && npm i && npm run test && npm run lint && npm run build-prod",
    "test": "jest --debug",
    "tsc": "tsc -d",
    "updateSnapshot": "npm run test -- --updateSnapshot",
    "watch": "npm run build-prod -- --watch",
    "watch-demo": "npm run build-demo -- --watch",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "FD",
    "FDMG",
    "Inputs",
    "TypeScript",
    "React"
  ],
  "author": "Willem Liu",
  "license": "ISC",
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.0",
    "@types/webpack-env": "^1.15.1",
    "awesome-typescript-loader": "^5.2.1",
    "chokidar": "^3.3.1",
    "coveralls": "^3.0.11",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.3.1",
    "tslint": "^5.20.1",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "date-input-polyfill": "^2.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0",
    "snyk": "^1.316.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "test/(.*)(test|spec)\\.(jsx?|tsx?)$",
    "setupFiles": [],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "coverageDirectory": "dist/coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "snyk": true
}
