{
    "name": "@fdmg/fd-inputs",
    "version": "2.0.3",
    "description": "FD-themed inputs",
    "types": "dist/fd-inputs.d.ts",
    "main": "dist/fd-inputs.js",
    "files": [
        "dist/"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:FDMediagroep/fd-ts-react-inputs.git"
    },
    "scripts": {
        "build-demo": "npm run copy-img && npm run copy-demo-html && cross-env NODE_ENV=production webpack --mode=production --config webpack.demo.config.js",
        "build-dev": "webpack --mode=development",
        "build-prod": "cross-env NODE_ENV=production webpack --mode=production",
        "copy-demo-html": "cpx -p -v \"demo/**/*.html\" \"dist/\"",
        "copy-img": "cpx -p -v \"img/**/*\" \"dist/\"",
        "lint": "tslint -p .",
        "release": "npm update && npm i && npm run test && npm run lint && npm run build-prod",
        "test": "jest --debug",
        "tsc": "tsc -d",
        "updateSnapshot": "npm run test -- --updateSnapshot",
        "watch": "npm run build-prod -- --watch",
        "watch-demo": "npm run build-demo -- --watch"
    },
    "keywords": [
        "FD",
        "FDMG",
        "Inputs",
        "TypeScript",
        "React"
    ],
    "author": "Willem Liu",
    "license": "ISC",
    "devDependencies": {
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^25.1.0",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.8.5",
        "@types/react-test-renderer": "^16.9.0",
        "@types/styled-components": "^5.0.0",
        "@types/webpack-env": "^1.14.0",
        "awesome-typescript-loader": "^5.2.1",
        "chokidar": "^3.0.2",
        "coveralls": "^3.0.6",
        "cpx": "^1.5.0",
        "cross-env": "^7.0.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "enzyme-to-json": "^3.4.0",
        "jest": "^24.9.0",
        "jest-cli": "^24.9.0",
        "react-test-renderer": "^16.9.0",
        "ts-jest": "^25.1.0",
        "tslint": "^5.18.0",
        "tslint-react": "^4.0.0",
        "typescript": "^3.5.3",
        "webpack": "^4.41.0",
        "webpack-cli": "^3.3.7"
    },
    "dependencies": {
        "date-input-polyfill": "^2.14.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "styled-components": "^5.0.0-beta.6-ej4"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "test/(.*)(test|spec)\\.(jsx?|tsx?)$",
        "setupFiles": [],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ],
        "collectCoverage": true,
        "coverageDirectory": "dist/coverage",
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "text-summary"
        ]
    }
}
